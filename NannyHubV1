game.StarterGui:SetCore("SendNotification",{
	Title = "NannyHub successfully loaded...",
	Text = "Have fun!",
	Duration = 5,
	Button1 = "Nice!"
})


local UserInputService = game:GetService("UserInputService")

function draggable(gui)
	local dragging
	local dragInput
	local dragStart
	local startPos

	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end

	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end

draggable(NannyHubFrame)
draggable(PlayerFrame)
draggable(MainFrame)
draggable(MiscFrame)


NannyButtonDestroy.MouseButton1Click:Connect(function()
	NannyHub:Destroy()
end)

NannyButtonMini.MouseButton1Click:Connect(function()
	NannyHubFrame.Visible = false
	OpenFrame.Visible = true
end)

OpenButton.MouseButton1Click:Connect(function()
	OpenFrame.Visible = false
	NannyHubFrame.Visible = true
end)


--Main

NannyMain.MouseButton1Click:Connect(function()
	NannyHubFrame.Visible = false
	MainFrame.Visible = true
end)

MainButtonMini.MouseButton1Click:Connect(function()
	MainFrame.Visible = false
	NannyHubFrame.Visible = true
end)

ScreamAplly.MouseButton1Click:Connect(function()
	game:GetService("ReplicatedStorage").ScreamWork:FireServer()
end)

--Player

NannyPlayer.MouseButton1Click:Connect(function()
	NannyHubFrame.Visible = false
	PlayerFrame.Visible = true
end)

PlayerButtonMini.MouseButton1Click:Connect(function()
	PlayerFrame.Visible = false
	NannyHubFrame.Visible = true
end)

SkinApplyBaby.MouseButton1Click:Connect(function()
	local Skin = SkinEnter.Text
	
	game:GetService("ReplicatedStorage").Equip:FireServer(Skin,"Baby")
end)

SkinApplyNany.MouseButton1Click:Connect(function()
	local Skin = SkinEnter.Text

	game:GetService("ReplicatedStorage").Equip:FireServer(Skin,"Nanny")
end)

SkinApplyMelee.MouseButton1Click:Connect(function()
	local Skin = SkinEnter.Text

	game:GetService("ReplicatedStorage").Equip:FireServer(Skin,"Melee")
end)

--Misc

NannyMisc.MouseButton1Click:Connect(function()
	NannyHubFrame.Visible = false
	MiscFrame.Visible = true
end)

MiscButtonMini.MouseButton1Click:Connect(function()
	MiscFrame.Visible = false
	NannyHubFrame.Visible = true
end)

InfAdminAplly.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
end)

